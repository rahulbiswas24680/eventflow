{
    "version": 2,
    "functions": {
        "api": {
            "runtime": "now-python@3.12",
            "memory": 512
        }
    },
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "rsvp/wsgi.py"
        }
    ]
}